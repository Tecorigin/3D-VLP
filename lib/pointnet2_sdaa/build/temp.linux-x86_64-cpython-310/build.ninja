  command = $tecocc $sdaa_cflags -c $in -o $out $sdaa_post_cflags
  depfile = $out.d
  deps = gcc

rule sdaa_link
  command = $tecocc $in -o $out $sdaa_ldflags

rule link
  command = $cxx $in -o $out $ldflags

